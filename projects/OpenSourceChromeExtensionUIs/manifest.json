{
  "manifest_version": 3,
  "name": "Open Source Chrome Extension UI",
  "description": "A modern starter UI for Chrome Extensions",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}